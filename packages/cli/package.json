  "dependencies": {
    "bunyan": "^1.8.15",
    "mongoose": "^6.8.2",
    "pg": "^8.8.0"
  },
  "devDependencies": {
    "@database-revisions/types": "file:../types",
    "@types/bunyan": "^1.8.8",
    "@types/pg": "^8.6.6",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eddiecorrigall/database-revisions.git"
  },
  "bugs": {
    "url": "https://github.com/eddiecorrigall/database-revisions/issues"
  },
  "homepage": "https://github.com/eddiecorrigall/database-revisions#readme"
