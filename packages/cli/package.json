{
  "private": false,
  "name": "@database-revisions/cli",
  "version": "0.3.0",
  "description": "A lightweight database migration tool for SQL and no-SQL databases",
  "author": "Eddie Corrigall <eddiecorrigall@gmail.com>",
  "license": "MIT",
  "keywords": [
    "db",
    "migration",
    "migrate",
    "revision",
    "version",
    "sql",
    "nosql",
    "mongoose",
    "mongodb",
    "postgres",
    "devops"
  ],
  "main": "build/cli/index.js",
  "bin": {
    "db": "./bin/cli.js",
    "database-revisions": "./bin/cli.js",
    "migrate": "./bin/cli.js",
    "revisions": "./bin/cli.js"
  },
  "engine": {
    "npm": "8.19.2",
    "node": "16.18.1"
  },
  "type": "commonjs",
  "scripts": {
