{
  "private": false,
  "name": "db-revisions",
  "version": "0.0.1",
  "description": "A lightweight database migration tool for SQL and no-SQL databases",
  "author": "Eddie Corrigall <eddiecorrigall@gmail.com>",
  "license": "MIT",
  "keywords": [
    "db",
    "migration",
    "migrate",
    "revision",
    "version",
    "sql",
    "nosql",
    "mongoose",
    "mongodb",
    "postgres",
    "devops"
  ],
  "main": "build/cli.js",
  "engine": {
    "npm": "8.19.2",
    "node": "16.18.1"
  },
  "type": "commonjs",
  "scripts": {
    "migrate": "node .",
    "preinstall": "npm audit --audit-level moderate > /dev/null",
    "test": "mocha",
    "clean": "rm -fr ./build",
    "lint": "npx eslint src",
    "build": "npx tsc --project ./tsconfig.build.json"
  },
  "dependencies": {
    "bunyan": "^1.8.15",
    "mongoose": "^6.8.2",
    "pg": "^8.8.0"
  },
  "devDependencies": {
    "@types/bunyan": "^1.8.8",
    "@types/mocha": "^10.0.1",
    "@types/pg": "^8.6.6",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-standard-with-typescript": "^26.0.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-n": "^15.6.0",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.2.0",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eddiecorrigall/node-revisions.git"
  },
  "bugs": {
    "url": "https://github.com/eddiecorrigall/node-revisions/issues"
  },
  "homepage": "https://github.com/eddiecorrigall/node-revisions#readme"
}
